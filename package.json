    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    
    "react-router-dom": "^6.8.0",
    "react-hook-form": "^7.43.0",
    "react-query": "^3.39.0",
    
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.12.0",
    "react-spring": "^9.7.0",
    
    "axios": "^1.4.0",
    "socket.io-client": "^4.6.0",
    "ws": "^8.13.0",
    
    "@stripe/stripe-js": "^1.52.0",
    "@stripe/react-stripe-js": "^2.1.0",
    "paypal-js": "^6.0.0",
    
    "chart.js": "^4.2.0",
    "react-chartjs-2": "^5.2.0",
    "recharts": "^2.5.0",
    "d3": "^7.8.0",
    
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    
    "@emailjs/browser": "^3.11.0",
    "react-speech-kit": "^3.0.1",
    "web-speech-api": "^0.0.1",
    
    "react-pdf": "^6.2.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "classnames": "^2.3.2",
    
    "react-intersection-observer": "^9.4.0",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.20",
    
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "react-transition-group": "^4.4.5",
    
    "focus-trap-react": "^10.1.0",
    "react-hotkeys-hook": "^4.4.0",
    "react-aria-live": "^3.0.4",
    
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.194",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.5.0",
    "testing-library/jest-dom": "^5.16.5",
    
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-import": "^2.27.5",
    
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "server": "nodemon server.js",
    
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "cross-env CI=true npm run test:coverage",
    
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:staging": "cross-env REACT_APP_ENV=staging npm run build",
    "build:production": "cross-env REACT_APP_ENV=production npm run build",
    
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=build",
    
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": "warn",
      "jsx-a11y/alt-text": "error",
      "jsx-a11y/aria-props": "error",
      "jsx-a11y/aria-proptypes": "error",
      "jsx-a11y/aria-unsupported-elements": "error",
      "jsx-a11y/role-has-required-aria-props": "error",
      "jsx-a11y/role-supports-aria-props": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "proxy": "http://localhost:3001",
  "homepage": "https://launchpadpoint.vercel.app"
}
